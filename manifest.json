{
    "manifest_version": 2,
    "name": "Dummy Popup",
    "version": "1.0.0",
  
    "description": "Dummy Poop",
    "icons": {"128": "plane.png"},
  
    "browser_action": {
        "default_icon": "plane.png"
             
    },
    
    "content_scripts": [
        {
            "matches":[
                "<all_urls>"
            ],
            "js":[
                "content.js"
            ]
        }
    ],

    "background":{
        "scripts": ["background.js"]
    },

    "permissions": ["activeTab"]
}